{
  "name": "antd-form-mate",
  "description": "基于 ant design 的表单组件",
  "version": "5.0.1",
  "main": "./dist/index.js",
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "antd": "^4.1.4",
